{
  "name": "chat-saas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3010",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "prisma generate && npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "wrangler d1 migrations apply chat-db"
  },
  "dependencies": {
    "@prisma/adapter-d1": "^5.22.0",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "^15.1.4",
    "postcss": "^8.4.49",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vercel": "^40.0.2",
    "wrangler": "^3.102.0"
  }
}
